#ifndef __MATH_FXI__
#define __MATH_FXI__

static const float PI = 3.141592f;

float sqr(float a) 
{
	return a * a;
}

void swap(inout float a, inout float b)
{
	float t = a;
	a = b;
	b = t;
}

uint wang_hash(uint seed)
{
    seed = (seed ^ 61) ^ (seed >> 16);
    seed *= 9;
    seed = seed ^ (seed >> 4);
    seed *= 0x27d4eb2d;
    seed = seed ^ (seed >> 15);
    return seed;
}


#endif